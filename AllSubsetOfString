package StringPackage;

public class AllSubsetOfString {
public static void main(String[] args) {
	String str="Funs";
	str=str.toUpperCase();//convert string into upperCase
	
System.out.println("All subset of FUNS :");
for(int i=0;i<str.length();i++)
	{   
	
	for(int j=i;j<str.length();j++)
	{
		System.out.println(str.substring(i, j+1));//in-built method substring is used to split the string 
	}
	}
}
}
